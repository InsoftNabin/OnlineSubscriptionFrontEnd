      asp-append-version="true"
    ></script>
    <script
      src="~/src/js/components/util.js"
      asp-append-version="true"
    ></script>
    <script
      src="~/src/js/components/image-input.js"
      asp-append-version="true"
    ></script>
  </head>
  <body>
    <div class="container">
      <main role="main" class="pb-3" id="Main">@RenderBody()</main>
    </div>

     <script
      src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"
