      asp-append-version="true"
    ></script>

    <script
      src="~/assets/js/scripts.bundle.js"
      asp-append-version="true"
    ></script>
   
    <script 
        src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.13/js/select2.min.js">
    </script>@RenderSection("scripts", required: false)
  </body>
</html>
